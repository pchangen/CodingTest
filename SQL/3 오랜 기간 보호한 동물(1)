# -- 코드를 입력하세요
# SELECT ANIMAL_INS.NAME, ANIMAL_INS.DATETIME
# FROM ANIMAL_INS LEFT JOIN ANIMAL_OUTS USING(ANIMAL_ID)
# WHERE ANIMAL_OUTS.ANIMAL_ID is null
# ORDER BY ANIMAL_INS.DATETIME LIMIT 3


# SELECT ANIMAL_INS.NAME, ANIMAL_INS.DATETIME 
# FROM ANIMAL_INS 
# WHERE ANIMAL_INS.ANIMAL_ID NOT IN (SELECT ANIMAL_ID FROM ANIMAL_OUTS) 
# ORDER BY ANIMAL_INS.DATETIME ASC LIMIT 3

SELECT I.NAME, I.DATETIME
FROM ANIMAL_INS I LEFT JOIN ANIMAL_OUTS O USING(ANIMAL_ID)
WHERE O.ANIMAL_ID is null
ORDER BY I.DATETIME LIMIT 3
